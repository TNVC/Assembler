test:
	./a.out
	./a.out code.asm
	./a.out temp.asm
	
	./a.out -in
	./a.out -in code.asm
	./a.out -in temp.asm
	
	./a.out -in code.asm -out out.code
	./a.out -out out.code -in temp.asm
	./a.out -out out.code -in code.asm
	
	./a.out -out out.code
	./a.out -save-temp -out out.code -in code.asm
	./a.out -out out.code -save-temp -in code.asm
	./a.out -out out.code -in code.asm -save-temp
	
	./a.out -in code.asm -in out.code
	./a.out -out out.code -out temp.asm
	
	./a.out -in code.asm -in out.code
	./a.out -out out.code -out temp.asm
